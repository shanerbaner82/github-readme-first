{
  "manifest_version": 3,
  "name": "Readme",
  "description": "Moves GitHub README above the code section",
  "version": "1.0.0",
  "permissions": [],
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
